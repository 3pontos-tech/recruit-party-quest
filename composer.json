{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "he4rt/sycorax",
    "type": "project",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "alizharb/filament-activity-log": "^1.2",
        "filament/filament": "^5.0",
        "filament/spatie-laravel-media-library-plugin": "^5.0",
        "he4rt/ai": ">=1",
        "he4rt/applications": ">=1",
        "he4rt/candidates": ">=1",
        "he4rt/core": ">=1",
        "he4rt/feedback": ">=1",
        "he4rt/location": ">=1",
        "he4rt/panel-admin": ">=1",
        "he4rt/panel-app": ">=1",
        "he4rt/panel-organization": ">=1",
        "he4rt/permissions": ">=1",
        "he4rt/recruitment": ">=1",
        "he4rt/screening": ">=1",
        "he4rt/teams": ">=1",
        "he4rt/users": ">=1",
        "internachi/modular": "dev-main#ad95fe9",
        "jeffgreco13/filament-breezy": "^3.1.0",
        "laravel/framework": "^12.48.1",
        "laravel/tinker": "^2.11.0",
        "monicahq/laravel-cloudflare": "^4.0.0",
        "owenvoke/blade-fontawesome": "^3.0.0",
        "prism-php/prism": "^0.99.13",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-backup": "^9.3.7",
        "spatie/laravel-permission": "^6.24",
        "tpetry/laravel-postgresql-enhanced": "^3.5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.3",
        "driftingly/rector-laravel": "^2.1.9",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.9.0",
        "laravel/boost": "^1.8.10",
        "laravel/pail": "^1.2.4",
        "laravel/pint": "^1.27.0",
        "laravel/telescope": "^5.16.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "pestphp/pest": "^4.3.1",
        "pestphp/pest-plugin-faker": "^4.0.0",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-livewire": "^4.1.0",
        "phpstan/extension-installer": "^1.4.3",
        "rector/rector": "^2.3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "npm install",
            "composer run-script post-root-package-install",
            "composer run-script post-create-project-cmd",
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi",
            "@php artisan modules:sync --no-phpunit --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "pint": "vendor/bin/pint --parallel",
        "test:pint": "vendor/bin/pint --test --parallel",
        "rector": "vendor/bin/rector",
        "test:rector": "vendor/bin/rector --dry-run",
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "test:phpstan": "vendor/bin/phpstan analyse --ansi",
        "test:unit": "vendor/bin/pest --parallel --compact --group=unit",
        "test:feature": "vendor/bin/pest --parallel --compact --group=feature",
        "test:browser": "vendor/bin/pest --parallel --compact --group=browser",
        "test:cov": "vendor/bin/pest --parallel --compact --coverage",
        "check": [
            "@test:rector",
            "@test:pint",
            "@test:phpstan"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "vendor/bin/pest --parallel --compact"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "@php artisan modules:sync --no-phpunit --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "bump-after-update": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "audit": {
            "abandoned": "report"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:gvieira18/modular"
        }
    ]
}
